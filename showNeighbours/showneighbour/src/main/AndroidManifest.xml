<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.cxyzy.vp.demos">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.cxyzy.vp.demos.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cxyzy.vp.demos.withFragment.demo1.ViewPagerActivity1" />
        <activity android:name="com.cxyzy.vp.demos.withFragment.demo2.ViewPagerActivity2" />
        <activity android:name="com.cxyzy.vp.demos.withPagerAdapter.demo3.ViewPagerActivity3" />
        <activity android:name=".withPagerAdapter.demo4.ViewPagerActivity4" />
    </application>

</manifest>